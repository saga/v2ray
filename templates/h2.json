{
	"port": 2333, // v2ray port
	"protocol": "vmess",
	"settings": {
		"clients": [
			{
				"id": "e55c8d17-2cf3-b21a-bcf1-eeacb011ed79", // uuid
				"level": 1,
				"alterId": 233 // alterid
			}
		]
	},
	"streamSettings": {
		"network": "h2",
		"httpSettings": {
			"host": [
				"saga.com" // domain
			],
			"path": "/saga" // path
		},
		"security": "tls",
		"tlsSettings": {
			"certificates": [
				{
					"certificateFile": "/usr/local/etc/v2ray/saga/v2ray/config/saga.com.cer",
					"keyFile": "/usr/local/etc/v2ray/saga/v2ray/config/saga.com.key"
				}
			]
		}
	},
	"sniffing": {
		"enabled": true,
		"destOverride": [
			"http",
			"tls"
		]
	}
}